{
    "name": "jwage/purl",
    "type": "library",
    "description": "URL Manipulation for PHP 5.3",
    "keywords": ["url", "manipulation"],
    "homepage": "http://github.com/jwage/purl",
    "license": "MIT",
    "authors": [
        {"name": "Jonathan H. Wage", "email": "jonwage@gmail.com"}
    ],
    "autoload": {
        "psr-4": {"Purl\\": "src/Purl"}
    },
    "autoload-dev": {
        "psr-4": {"Purl\\Test\\": "tests/Purl/Test"}
    },
    "config": {
        "sort-packages": true
    },
    "require": {
    	"php": "^7.1",
        "jeremykendall/php-domain-parser": "^1.3.1"
    },
    "require-dev": {
        "doctrine/coding-standard": "^4.0",
        "phpstan/phpstan": "^0.9.2",
        "phpstan/phpstan-strict-rules": "^0.9",
        "phpunit/phpunit": "^5.5|^6.5|^7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
